CONFIG {
  WIDTH 4
  HEIGHT 3

  LEARNING_RATE 0.1
  LEARNING_LEAK 0.1

  PREDICTION_DELAY 3

  AMPLIFY mse
}

NODE I0 0,1 INPUT
NODE I1 0,2 INPUT
NODE F0 1,1
NODE F1 1,2
NODE S0 2,1
NODE S1 2,2
NODE D0 2,0
NODE D1 2,3
NODE T0 3,1
NODE T1 3,2
NODE O0 4,1 OUTPUT
NODE O1 4,2 OUTPUT

FLOW I0 F0 1
FLOW F0 S0 1
FLOW S0 T0 1
FLOW F0 D0 1
FLOW F0 S1 1
FLOW D0 T0 1
FLOW T0 O0 1

FLOW I1 F1 1
FLOW F1 S1 1
FLOW S1 T1 1
FLOW F1 D1 1
FLOW F1 S0 1
FLOW D1 T1 1
FLOW T1 O1 1

DATA {
  `1,-1` `-1,1`
  `0.9,-1.1` `-1,1`
  `1.1,-0.9` `-1,1`
}
