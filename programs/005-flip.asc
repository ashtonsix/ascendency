SEED `Date.now()`

CONFIG {
  WIDTH  4
  HEIGHT 3
  PREDICTION_DELAY 4

  ACTIVATE      sigmoid
  AMPLIFY       mse
  AMPLITUDE     0.1
  TRANSFER_RATE 0.5
  CYCLE_ASPECT  0
  CYCLE_LEAK    0
  VALUE_DECAY   0
  SLOPE_DECAY   0
}

NODE IN_0 0,1 INPUT
NODE IN_1 0,2 INPUT
NODE SPLIT_0 1,1
NODE SPLIT_1 1,2
NODE OUTSIDE_0 2,0
NODE OUTSIDE_1 2,3
NODE UNCROSS_0 2,1
NODE UNCROSS_1 2,2
NODE JOIN_0 3,1
NODE JOIN_1 3,2
NODE OUT_0 4,1 OUTPUT
NODE OUT_1 4,2 OUTPUT

FLOW IN_0 SPLIT_0 1
FLOW SPLIT_0 OUTSIDE_0 1
FLOW SPLIT_0 UNCROSS_0 1
FLOW SPLIT_0 UNCROSS_1 1
FLOW UNCROSS_0 JOIN_0 1
FLOW OUTSIDE_0 JOIN_0 1
FLOW JOIN_0 OUT_0 1

FLOW IN_1 SPLIT_1 1
FLOW SPLIT_1 OUTSIDE_1 1
FLOW SPLIT_1 UNCROSS_1 1
FLOW SPLIT_1 UNCROSS_0 1
FLOW UNCROSS_1 JOIN_1 1
FLOW OUTSIDE_1 JOIN_1 1
FLOW JOIN_1 OUT_1 1

DATA {
  `1,-1` `-1,1`
}
